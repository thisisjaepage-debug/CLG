        const scholars = scholarData.scholars;
        const allInterests = scholarData.all_interests;
        const autocompleteInterests = scholarData.autocomplete_interests;
        
        const searchInput = document.getElementById('searchInput');
        const searchBtn = document.getElementById('searchBtn');
        const resetBtn = document.getElementById('resetBtn');
        const results = document.getElementById('results');
        const stats = document.getElementById('stats');
        const statsText = document.getElementById('statsText');
        const suggestionsDiv = document.getElementById('suggestions');
        
        // Special search terms that show all scholars
        const showAllTerms = ['justice'];
        
        // Autocomplete functionality
        searchInput.addEventListener('input', function() {
            const query = this.value.toLowerCase().trim();
            suggestionsDiv.innerHTML = '';
            
            if (query.length === 0) {
                suggestionsDiv.style.display = 'none';
                return;
            }
            
            const matches = autocompleteInterests.filter(interest => 
                interest.includes(query)
            ).slice(0, 8);
            
            if (matches.length > 0) {
                matches.forEach(interest => {
                    const div = document.createElement('div');
                    div.className = 'suggestion-item';
                    div.textContent = interest;
                    div.addEventListener('click', function() {
                        searchInput.value = interest;
                        suggestionsDiv.style.display = 'none';
                        performSearch();
                    });
                    suggestionsDiv.appendChild(div);
                });
                suggestionsDiv.style.display = 'block';
            } else {
                suggestionsDiv.style.display = 'none';
            }
        });
        
        // Hide suggestions when clicking outside
        document.addEventListener('click', function(e) {
            if (!searchInput.contains(e.target) && !suggestionsDiv.contains(e.target)) {
                suggestionsDiv.style.display = 'none';
            }
        });
        
        // Search on Enter key
        searchInput.addEventListener('keypress', function(e) {
            if (e.key === 'Enter') {
                suggestionsDiv.style.display = 'none';
                performSearch();
            }
        });
        
        // Search button click
        searchBtn.addEventListener('click', performSearch);
        
        // Reset button click
        resetBtn.addEventListener('click', function() {
            searchInput.value = '';
            results.innerHTML = '';
            stats.style.display = 'none';
            suggestionsDiv.style.display = 'none';
        });
        
        function performSearch() {
            const query = searchInput.value.toLowerCase().trim();
            
            if (query === '') {
                results.innerHTML = '<div class="no-results">Please enter a research interest to begin your search.</div>';
                stats.style.display = 'none';
                return;
            }
            
            // Check if query is a special "show all" term
            let matches;
            if (showAllTerms.includes(query)) {
                // Show all scholars for special terms like "justice"
                matches = scholars;
            } else {
                // Normal search - find matching scholars
                matches = scholars.filter(scholar => 
                    scholar.interests.some(interest => 
                        interest.includes(query)
                    )
                );
            }
            
            // Display results
            if (matches.length === 0) {
                results.innerHTML = '<div class="no-results">No scholars found for "' + escapeHtml(query) + '". Please try another research interest.</div>';
                stats.style.display = 'none';
            } else {
                // Show stats
                stats.style.display = 'block';
                statsText.textContent = matches.length + ' scholar' + (matches.length === 1 ? '' : 's') + ' found for "' + query + '"';
                
                // Display scholar cards
                results.innerHTML = matches.map(scholar => {
                    const pubLink = scholar.publications ? 
                        `<div class="scholar-publications">
                            <a href="${escapeHtml(scholar.publications)}" target="_blank" rel="noopener noreferrer" class="publications-link">
                                View Publications →
                            </a>
                        </div>` : '';
                    
                    return `
                        <div class="scholar-card">
                            <div class="scholar-name">${escapeHtml(scholar.name)}</div>
                            <div class="scholar-university">${escapeHtml(scholar.university)}</div>
                            <div class="scholar-interests">
                                <span class="interest-label">Research Interests:</span> ${highlightMatch(scholar.interests_display, query, showAllTerms.includes(query))}
                            </div>
                            ${pubLink}
                        </div>
                    `;
                }).join('');
            }
        }
        
        function escapeHtml(text) {
            const div = document.createElement('div');
            div.textContent = text;
            return div.innerHTML;
        }
        
        function highlightMatch(text, query, skipHighlight) {
            const escaped = escapeHtml(text);
            // Don't highlight if it's a special "show all" term
            if (skipHighlight) {
                return escaped;
            }
            const regex = new RegExp('(' + escapeRegex(query) + ')', 'gi');
            return escaped.replace(regex, '<span class="highlight">$1</span>');
        }
        
        function escapeRegex(string) {
            return string.replace(/[.*+?^${}()|[\]\\]/g, '\\$&');
        }
    </script>
</body>
</html>
